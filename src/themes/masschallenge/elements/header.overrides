.ui.normal.header {
    font-weight: normal;
}

.ui.clickable.header {
  cursor: pointer;
}

.ui.clickable.header:hover{
  color: @itemTextColor;
}

.ui.header > .fullWidth.content {
    width: 100%;
}

.ui.darker.header {
  color: @itemTextColor;
}

.ui.header .sub.header.sub-title {
  font-weight: 600;
  font-size: 14px;
  line-height: 19px;
  color: @midGray;
}

.sub.header.small {
  font-size: 80%;
}
