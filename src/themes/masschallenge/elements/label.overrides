.ui.label.card-program {
  color: @itemTextColor;
  margin: 0.5em;
  padding: 0.5em;

  @media only screen and (max-width: 767px) {
    font-size: 0.75em;
  }
  @media only screen and (min-width: 768px) {
    font-size: 0.65em;
    line-height: 0.75em;
  }
}

.ui.dark.label{
  background: @darkPrimaryColor;
  color: @whiteColor;
}
